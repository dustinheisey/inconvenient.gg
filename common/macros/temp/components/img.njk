<div class="{{class}} frame {% if props.style.ar %}{{props.style.ar}}{% endif %}">
    {% if props.variant === 'avatar' %}
        <img src="https://i.pravatar.cc/300" alt="avatar">
    {% else %}
        {% set src = './public/img/' + props.src%}
        {% img src, props.alt %}
    {% endif %}
</div>