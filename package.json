{
  "name": "inconvenient.gg",
  "private": true,
  "version": "1.0.0",
  "description": "Website for 'An Inconvenient Modpack'",
  "repository": {
    "type": "git",
    "url": "https://github.com/dustinheisey/inconvenient.gg.git"
  },
  "scripts": {
    "lint:js": "npx eslint **/*.js --fix",
    "lint:css": "cd styles && npx stylelint \"**/*.css\" --fix",
    "lint": "npm run lint:js && npm run lint:css",
    "format": "prettier \"**/*.{cjs,html,js,json,md,njk,css}\" --ignore-path ./.eslintignore --write",
    "sb": "storybook dev -c common/docs/.storybook",
    "styles": "postcss styles/style.css -o styles/style.min.css --watch",
    "build:js": "rollup -c rollup.config.js",
    "start": "npm run build:js && cd site && rm -rf site/ && npx @11ty/eleventy --serve --incremental",
    "build:site": "rollup -c rollup.config.js && npm run build:js && cd site && rm -rf sites/ && npx @11ty/eleventy"
  },
  "author": {
    "name": "Dustin Heisey"
  },
  "devDependencies": {
    "@11ty/eleventy": "^2.0.1",
    "@11ty/eleventy-img": "^4.0.2",
    "@11ty/eleventy-navigation": "^0.3.5",
    "@11ty/eleventy-plugin-inclusive-language": "^1.0.3",
    "@11ty/eleventy-plugin-syntaxhighlight": "^5.0.0",
    "@etchteam/storybook-addon-status": "^5.0.0",
    "@jackdbd/eleventy-plugin-content-security-policy": "^1.0.0",
    "@quasibit/eleventy-plugin-sitemap": "^2.2.0",
    "@rollup/plugin-terser": "^0.4.4",
    "@storybook/addon-a11y": "^8.2.9",
    "@storybook/addon-essentials": "^8.2.9",
    "@storybook/blocks": "^8.2.9",
    "@storybook/html-webpack5": "^8.2.9",
    "a11y-sitechecker": "^6.1.0",
    "cssnano": "^7.0.5",
    "eleventy-multisite": "^0.1.1",
    "eleventy-plugin-gen-favicons": "^1.1.3",
    "eleventy-plugin-pwa-v2": "^1.0.2",
    "eleventy-plugin-svg-sprite": "^2.4.2",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-storybook": "^0.8.0",
    "html-minifier": "^4.0.0",
    "luxon": "^3.5.0",
    "markdown-it": "^14.1.0",
    "markdown-it-anchor": "^9.0.1",
    "postcss": "^8.4.41",
    "postcss-cli": "^11.0.0",
    "postcss-easy-import": "^4.0.0",
    "postcss-preset-env": "^10.0.1",
    "prettier": "^3.3.3",
    "purgecss": "^6.0.0",
    "purgecss-from-html": "^6.0.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "simple-nunjucks-loader": "^3.2.0",
    "storybook": "^8.2.9",
    "storybook-dark-mode": "^4.0.2",
    "stylelint-config-standard": "^33.0.0",
    "util": "^0.12.5"
  },
  "engines": {
    "node": ">=12.0.0",
    "npm": ">=6.0.0"
  }
}
