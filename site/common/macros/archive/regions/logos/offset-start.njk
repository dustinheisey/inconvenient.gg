<div class="sidebar region sidebar-split-s align-start gap-xl theme-{{props.theme or 'background'}} {{props.classes}}">
    <section class="prose fixed">
        <p class="overline">{{props.content.overline}}</p>
        <h1 class="page-headline">
            {{props.content.headline}}
        </h1>
    </section>
    <div class="grid grid-{{props.col}}">
        {% for logo in props.content.logos %}
        <div class="frame photo">
            {% img logo.src, logo.alt %}
        </div>
        {% endfor %}
    </div>
</div>